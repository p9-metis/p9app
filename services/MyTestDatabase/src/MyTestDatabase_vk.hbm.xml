    <property name="column3" type="java.lang.String">
      <column name="`COLUMN3`" length="255" not-null="false" precision="19"/>
    </property>
    <property name="column4" type="java.lang.String">
      <column name="`COLUMN4`" length="255" not-null="false" precision="19"/>
    </property>
  </class>
</hibernate-mapping>